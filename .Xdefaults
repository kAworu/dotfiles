URxvt.termName:         rxvt
URxvt.depth:            32
URxvt.title:            Terminal
URxvt.background:       rgba:0011/1111/1111/EEEE
URxvt.foreground:       rgba:DDDD/DDDD/DDDD/FFFF
URxvt.font:             xft:Liberation Mono:style=Regular:size=9:antialias=true
XFontSel*background:    black
XFontSel*foreground:    black
URxvt.loginShell:       true
