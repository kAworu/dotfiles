XTerm*termName:       xterm-256color
XTerm*foreground:       #ebdbb2
XTerm*background:       #282828
XTerm*FaceName:         monospace:size=12:antialias=true
!XTerm*FaceName:         Fixed:pixelsize=14:antialias=true
XTerm*faceSize:         8
XTerm*metaSendsEscape:  true
XTerm*loginShell:       true
XTerm*title:            Terminal
XTerm*utmpInhibit:      False
XTerm*on2Clicks:        word
XTerm*on3Clicks:        line
XTerm*on4Clicks:        page
XTerm*on5Clicks:        all
XTerm*saveLines:        1024

Xcursor.theme: whiteglass
Xcursor.size: 16
Xft.dpi: 98

# soft contrast: background = #32302f
# medium contrast: background = #282828
# hard contrast: background = #1d2021
*background:            #282828
*foreground:            #ebdbb2
*fadeColor:             #1d2021
*cursorColor:           #ebdbb2
*pointerColorBackground:#32302f
*pointerColorForeground:#ebdbb2
# dark0 + gray
*color0: #282828
*color8: #928374
# neutral_red + bright_red
*color1: #cc241d
*color9: #fb4934
# neutral_green + bright_green
*color2:  #98971a
*color10: #b8bb26
# neutral_yellow + bright_yellow
*color3:  #d79921
*color11: #fabd2f
# neutral_blue + bright_blue
*color4:  #458588
*color12: #83a598
# neutral_magenta + bright_magenta
*color5:  #b16286
*color13: #d3869b
# neutral_cyan + faded_cyan
*color6:  #689d6a
*color14: #8ec07c
# light4 + light1
*color7:  #a89984
*color15: #ebdbb2
