URxvt.termName:         rxvt
URxvt.depth:            32
URxvt.title:            Terminal
URxvt.background:       rgba:0000/1717/1c1c/EEEE
URxvt.foreground:       rgba:DDDD/DDDD/DDDD/FFFF
URxvt.font:             xft:Droid Sans Mono:style=Regular:size=9:antialias=true
XFontSel*background:    black
XFontSel*foreground:    black
URxvt.loginShell:       true
URxvt.modifier:         alt
URxvt.borderLess:       false

URxvt.saveLines:        1024
URxvt.jumpScroll:       true
URxvt.scrollstyle:      plain
URxvt.scrollBar:        false
URxvt.scrollColor:      #444444
URxvt.scrollTtyOutput:  false
URxvt.scrollWithBuffer: true
URxvt.scrollTtyKeypress:true

URxvt.utmpInhibit: false
URxvt.perl-ext-common:  keyboard-select,url-select,clipboard
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-o: perl:url-select:select_next
Urxvt.url-select.autocopy: true
URxvt.url-select.underline: true
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped

URxvt.matcher.button:   1
URxvt.cursorUnderline:  false
URxvt.cursorBlink:      false
URxvt.cursorColor: #657b83

! Fix font space
URxvt*letterSpace: -1

URxvt.fading:           25%
